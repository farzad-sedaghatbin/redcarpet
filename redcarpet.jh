entity Merchant {
	name String,
	type MerchatType,
	range ClassType

}

entity Order {
	guestNo Integer,
	cermonyType CermonyType,
	location String,
	description String,
	budget String

}
enum ClassType {
    LOW, MID, HIGH, LUXURY
}
enum MerchatType {
    MUSIC
}
enum MarketType {
    SUBSCRIPTION, ADS
}
enum CermonyType {
    WEDDING, BIRTHDAY, ENGAGE
}